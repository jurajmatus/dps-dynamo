{
  "data_dir": "/tmp/consul",
  "log_level": "INFO",
  "server": false,
  "advertise_addr": "127.0.0.1",
  "bind_addr": "127.0.0.1",
  "client_addr": "127.0.0.1",
  "start_join": [],

  "check": {
    "id": "grafana-dashboard",
    "name": "HTTP API on port 80",
    "http": "http://localhost:80",
    "interval": "5s",
    "timeout": "3s"
  },

  "check": {
    "id": "graphite",
    "name": "Graphite on port 2003",
    "tcp": "localhost:2003",
    "interval": "5s",
    "timeout": "3s"
  },

  "check": {
    "id": "rsyslog",
    "name": "Syslog on port 514",
    "tcp": "localhost:514",
    "interval": "5s",
    "timeout": "3s"
  }


}
